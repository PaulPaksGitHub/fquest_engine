scene way_to_secret_flat {
    clean ch
    clean bgOverlay
    clean dialog

    pause music
    play ambient "ambient/steps_indoor.mp3"
    show bg sprite _corridor : (assetPath: "background/curator_office/corridor.png")

    wait

    play ambient "ambient/car_engine.mp3"
    show bg sprite _corridor : (assetPath: "background/car/inside.png")

    wait
    narrator "Конец..."
        - "Заново" {
            jump start
        }
}